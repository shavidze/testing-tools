{
    "parserOptions": {
        "ecmaVersion":2021,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    
    "rules": {
        "strict":["error", "never"],
        "valid-typeof":"warn",
        "no-unsafe-negation":"error",
        "no-unused-vars":"error",
        "no-unexpected-multiline":"error",
        "no-undef":"error"
    },
    "env": {
        "browser":true
    }
}